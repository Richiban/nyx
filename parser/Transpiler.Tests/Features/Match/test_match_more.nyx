module MatchMore

def listEnd = match values
  | [1, 2, ...rest] -> rest
  | else -> []

def tupleCase = match pair
  | (1, x) -> x
  | else -> 0

def rangeCase = match value
  | 1..3 -> 1
  | else -> 0

def tagInline = match result
  | #ok data -> data
  | #error msg -> "Error: {msg}"
  | else -> "unexpected"

def guardCase = match value
  | > 10 -> "big"
  | else -> "small"
