def list = [1, 2, 3, 4, 5]

def processWithSplat = match list
  | [first, ...rest] -> "first: {first}"
  | [...] -> "all ignored"
  | else -> "empty"

def getFirst = match list
  | [x, ...] -> x
  | else -> 0

def nested = [[1, 2], [3, 4, 5]]

def processNested = match nested
  | [[a, b], [c, ...rest]] -> a + b + c
  | else -> 0
