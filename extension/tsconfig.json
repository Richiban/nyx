{
  "compilerOptions": {
    "module": "commonjs",
  "moduleResolution": "node",
    "target": "ES2020",
    "outDir": "dist",
    "lib": ["ES2020"],
    "rootDir": "src",
    "types": ["node", "vscode"],
  "typeRoots": ["./node_modules/@types"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "include": ["src"]
}
