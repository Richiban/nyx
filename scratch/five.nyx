
def f: int -> string = {
    | 1 -> "one"
    | _ -> "many"
}

context Console = (
    println: a -> ()
)

context CAdd = (
    add: (int, int) -> int
)

def g: [Console + CAdd] = { s ->
    def r = add(4, 5)
    println(s)
    println(r)
}

def h = {
    use Console(
        println = { s -> () }
    )

    use CAdd(
        add = { x, y -> x + y }
    )

    g("Hello world")
}

h()