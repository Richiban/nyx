def testRange = match 5 | 1..10 -> "small" | 11..100 -> "medium" | else -> "large"

def testGuard = match 25 | > 100 -> "huge" | > 10 -> "big" | < 0 -> "negative" | else -> "small"

def testTag = match optValue | #some(x) -> x | #none -> 0 | else -> -1

def testMixed = match value
  | 0 -> "zero"
  | 1..5 -> "tiny"
  | > 100 -> "huge"
  | #error(msg) -> msg
  | else -> "normal"